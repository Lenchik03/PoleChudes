<Window x:Class="PoleChudes.NickNameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PoleChudes"
        mc:Ignorable="d"
        Title="NickNameWindow" Height="100" Width="150">
    <StackPanel>
        <Label Content="Укажите NickName"/>
        <TextBox Text="{Binding NickName}"/>
        <Button Content="Ок" Width="50" Margin="7" Click="Send"/>
    </StackPanel>
</Window>
